<h1>管理画面のユーザ一覧画面</h1>

<table>
  <ul>
    <tr>
      <th><%= "ユーザーID" %></th>
      <th><%= "ユーザー名" %></th>
      <th><%= "ユーザ登録日" %></th>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
      </tr>
    <% end %>
  </ul>
</table>